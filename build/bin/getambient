#!/usr/bin/env bash
function getambient() {
local no_kiali
local no_prometheus
local no_grafana
local no_metallb
local no_cluster
local no_bookinfo
local no_gateway


print_help() {
    printf "getambient

Options:\n"

printf '\t%s\n' '--no-kiali: disable Kiali [off]'
printf '\t%s\n' '--no-prometheus: disable Prometheus [off]'
printf '\t%s\n' '--no-grafana: do not deploy Grafana [off]'
printf '\t%s\n' '--no-metallb: disable MetalLB [off]'
printf '\t%s\n' '--no-cluster: use existing cluster [off]'
printf '\t%s\n' '--no-bookinfo: do not deploy bookinfo [off]'
printf '\t%s\n' '--no-gateway: do not deploy gateway [off]'
}
die()
{
	test "${_PRINT_HELP:-no}" = yes && print_help >&2
	echo "$1" >&2
	return 1
}
_positionals=()
parse_commandline() {
	_positionals_count=0
	while test $# -gt 0
	do
		_key="$1"
		case "$_key" in
            --no-kiali)
    [[ $no_kiali == "" ]] || { die "Flag already set 'no_kiali'." || return 1; }
    
    no_kiali="on"
;;
--no-kiali=*)
    [[ $no_kiali == "" ]] || { die "Flag already set 'no_kiali'." || return 1; }
    no_kiali="${_key##--no-kiali=}"
;;

--no-prometheus)
    [[ $no_prometheus == "" ]] || { die "Flag already set 'no_prometheus'." || return 1; }
    
    no_prometheus="on"
;;
--no-prometheus=*)
    [[ $no_prometheus == "" ]] || { die "Flag already set 'no_prometheus'." || return 1; }
    no_prometheus="${_key##--no-prometheus=}"
;;

--no-grafana)
    [[ $no_grafana == "" ]] || { die "Flag already set 'no_grafana'." || return 1; }
    
    no_grafana="on"
;;
--no-grafana=*)
    [[ $no_grafana == "" ]] || { die "Flag already set 'no_grafana'." || return 1; }
    no_grafana="${_key##--no-grafana=}"
;;

--no-metallb)
    [[ $no_metallb == "" ]] || { die "Flag already set 'no_metallb'." || return 1; }
    
    no_metallb="on"
;;
--no-metallb=*)
    [[ $no_metallb == "" ]] || { die "Flag already set 'no_metallb'." || return 1; }
    no_metallb="${_key##--no-metallb=}"
;;

--no-cluster)
    [[ $no_cluster == "" ]] || { die "Flag already set 'no_cluster'." || return 1; }
    
    no_cluster="on"
;;
--no-cluster=*)
    [[ $no_cluster == "" ]] || { die "Flag already set 'no_cluster'." || return 1; }
    no_cluster="${_key##--no-cluster=}"
;;

--no-bookinfo)
    [[ $no_bookinfo == "" ]] || { die "Flag already set 'no_bookinfo'." || return 1; }
    
    no_bookinfo="on"
;;
--no-bookinfo=*)
    [[ $no_bookinfo == "" ]] || { die "Flag already set 'no_bookinfo'." || return 1; }
    no_bookinfo="${_key##--no-bookinfo=}"
;;

--no-gateway)
    [[ $no_gateway == "" ]] || { die "Flag already set 'no_gateway'." || return 1; }
    
    no_gateway="on"
;;
--no-gateway=*)
    [[ $no_gateway == "" ]] || { die "Flag already set 'no_gateway'." || return 1; }
    no_gateway="${_key##--no-gateway=}"
;;

			-h|--help|-h*)
				print_help
                return 2
				;;
            -*)
                die "Unknown flag ${_key}"
                ;;
			*)
				_last_positional="$1"
				_positionals+=("$_last_positional")
				_positionals_count=$((_positionals_count + 1))
				;;
		esac
		shift
	done
	[[ ${no_kiali:-} == "" ]] && { no_kiali=off; }
[[ ${no_prometheus:-} == "" ]] && { no_prometheus=off; }
[[ ${no_grafana:-} == "" ]] && { no_grafana=off; }
[[ ${no_metallb:-} == "" ]] && { no_metallb=off; }
[[ ${no_cluster:-} == "" ]] && { no_cluster=off; }
[[ ${no_bookinfo:-} == "" ]] && { no_bookinfo=off; }
[[ ${no_gateway:-} == "" ]] && { no_gateway=off; }

	return 0
}

parse_commandline "$@"
ret=$?
if [[ $ret == 2 ]]; then
    return 0
fi
if [[ $ret != 0 ]]; then
    return $ret
fi
set -- "${_positionals[@]}"

if [[ "${no_cluster}" != "on" ]]; then
  build_kind
fi

